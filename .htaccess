<FilesMatch "\.(ico|jpg|jpeg|png|gif|js|css|swf|svg|pdf|flv|mp3)$">
<IfModule mod_expires.c>
 ExpiresActive on
 ExpiresDefault "access plus 1 month 2 days 3 hours"
 Header set Cache-Control "public"
</IfModule>
</FilesMatch>
